<!DOCTYPE html>

<html lang="zh-cn">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="description" content="MartPlace - Complete Online Multipurpose Marketplace HTML Template">
    <meta name="keywords" content="marketplace, easy digital download, digital product, digital, html5">
    <!-- CSS  -->
    <link href="~/Content/animate.css" rel="stylesheet" />
    <link href="~/Content/font-awesome.min.css" rel="stylesheet" />
    <link href="~/Content/fontello.css" rel="stylesheet" />
    <link href="~/Content/jquery-ui.css" rel="stylesheet" />
    <link href="~/Content/lnr-icon.css" rel="stylesheet" />
    <link href="~/Content/owl.carousel.css" rel="stylesheet" />
    <link href="~/Content/slick.css" rel="stylesheet" />
    <link href="~/Content/trumbowyg.min.css" rel="stylesheet" />
    <link href="~/Content/bootstrap.min.css" rel="stylesheet" />
    <link href="~/Content/style.css" rel="stylesheet" />
    <!-- END CSS -->
    <!-- ICON -->
    <link rel="icon" type="image/png" sizes="16*16" href="~/images/favicon.png" />


    <title>@ViewBag.Title</title>
</head>
<body class="preload home1 mutlti-vendor">
    <div class="menu-area">
        <!--顶部栏-->
        <div class="top-menu-area">
            <div class="container">
                <div class="row">
                    <!--Logo-->
                    <div class="col-lg-3 col-md-3 col-6 v_middle">
                        <div class="logo">
                            <a href="index.html">
                                <img src="~/images/logo.png" alt="logo image" class="img-fluid">
                            </a>
                        </div>
                    </div>
                    <!--用户个人信息-->
                    @Html.Action("UserInfo", "Account");
                </div>
            </div>
        </div>
    </div>

    <!--导航-->
    @RenderSection("Nav")


    @RenderBody()


    <!--页尾底栏-->
    @Html.Partial("PartialLayout/Footer")
    <!--JS-->
    @Html.Partial("cssJS/jsBody")
</body>
</html>
